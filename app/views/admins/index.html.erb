<% @title="All Admins" %>
<div class="container mt-5">
  <h1>View all Admins</h1>
  <%= link_to("Create new admin", new_admin_path()) %>
  <br/><br/>
  <table class="table">
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th>Actions</th>
    </tr>
    <% @admins.each do |admin| %>
      <tr>
        <td><%= admin.username %></td>
        <td><%= admin.email %></td>
        <td>
          <%= link_to('Show', admin_path(admin)) %>
          <%= link_to('Edit', edit_admin_path(admin)) %>
          <%= link_to('Delete', delete_admin_path(admin)) %>
        </td>
      </tr>
    <% end %>
  </table>
</div>
