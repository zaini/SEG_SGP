<h1>Admin#index</h1>
<p>Listing all admins</p>
<%= link_to("Create new admin", new_admin_path()) %>
<table class="table">
  <tr>
    <th>Username</th>
    <th>Email</th>
    <th>Password</th>
    <th>Actions</th>
  </tr>
  <% @admins.each do |admin| %>
    <tr>
      <td><%= admin.username %></td>
      <td><%= admin.email %></td>
      <td><%= admin.password %></td>
      <td>
        <%= link_to('Show', admin_path(admin)) %>
        <%= link_to('Edit', edit_admin_path(admin)) %>
        <%= link_to('Delete', delete_admin_path(admin)) %>
      </td>
    </tr>
  <% end %>
</table>
